from multiprocessing import Process
import time
def method1():
    print ("in method1")
    time.sleep(5)
    print ("in method1")

def method2():
    print ("in method2")
    time.sleep(5)
    print ("in method2")
    
begin = time.time()

p1 = Process(target=method1)
p2 = Process(target=method2)# create a process object p1
p1.start()                   # starts the process p1
p2.start()
p1.join()
p2.join()
end = time.time()
  
# total time taken
print(f"Total runtime of the program is {end - begin}")
